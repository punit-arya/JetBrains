<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="19" />
    <option name="FONT_FAMILY" value="M+ 1m light" />
    <option name="SECONDARY_FONT_FAMILY" value="DejaVu Sans Mono" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="HTML" value="true" />
        <entry key="JSON" value="true" />
        <entry key="Python" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_BLOCK_CURSOR" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bHIG:">
      <option name="FOREGROUND" value="7f0000" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bNOT:">
      <option name="FOREGROUND" value="bfbf00" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bFIN:">
      <option name="FOREGROUND" value="bf00" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bSCR:">
      <option name="FOREGROUND" value="5f5f5f" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bMED:">
      <option name="FOREGROUND" value="bf0000" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bVER:">
      <option name="FOREGROUND" value="7fff00" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bLOW:">
      <option name="FOREGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bWAI">
      <option name="FOREGROUND" value="ffffff" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bDOI">
      <option name="FOREGROUND" value="ff5f00" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3f3f3f" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="autoCloseTag" value="false" />
    <option name="automaticallyInsertClosingTag" value="false" />
    <option name="automaticallyInsertRequiredAttributes" value="false" />
    <option name="automaticallyInsertRequiredSubTags" value="false" />
    <option name="automaticallyStartAttribute" value="false" />
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="8" />
  </component>
</application>