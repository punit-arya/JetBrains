    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bHIG\b.*">
      <option name="FOREGROUND" value="7f0000" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bMED\b.*">
      <option name="FOREGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bLOW\b.*">
      <option name="FOREGROUND" value="ff7f00" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bVER\b.*">
      <option name="FOREGROUND" value="7fff7f" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bFIN\b.*">
      <option name="FOREGROUND" value="ff00" />
      <option name="FONT_TYPE" value="2" />
